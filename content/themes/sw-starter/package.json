{
  "name": "sw-starter",
  "version": "1.0.0",
  "description": "Stater WP theme created by Sideways NYC",
  "repository": "https://github.com/williameliel/sw-starter",
  "main": "index.js",
  "scripts": {
    "images": "cp -r assets/images dist/.",
    "fonts" : "cp -r assets/fonts dist/.",
    "uglify": "uglifyjs dist/index.js -o dist/index.min.js --compress=sequences,properties,dead_code,conditionals,comparisons,booleans,loops,unused,if_return,join_vars,warnings,cascade,collapse_vars,drop_console --mangle",
    "uglifycss": "uglifycss dist/main.css > dist/main.min.css",
    "minify": "minify --output dist/ dist/main.css",
    "dev": "npm run images && webpack --progress --colors --watch",
    "build": "npm run images && webpack && npm run uglify && npm run uglifycss"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "babel-core": "6.7.7",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "bootstrap-sass": "^3.3.7",
    "browser-sync": "^2.12.5",
    "browser-sync-webpack-plugin": "^1.0.1",
    "css-loader": "^0.23.0",
    "enquire.js": "^2.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flag-icon-css": "^2.3.0",
    "google-maps-api-loader": "^0.2.0",
    "jquery": "^2.2.3",
    "jquery-validation": "^1.15.0",
    "jssocials": "^1.3.1",
    "minifier": "^0.8.0",
    "node-sass": "^3.4.2",
    "nprogress": "^0.2.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "path": "^0.12.7",
    "pickadate": "^3.5.6",
    "postcss-loader": "^0.8.2",
    "sass-loader": "^3.1.2",
    "scrollmagic": "^2.0.5",
    "select2": "^4.0.3",
    "simple-modal-es6": "0.0.9",
    "slick-carousel": "^1.5.9",
    "smoothstate": "^0.7.2",
    "style-loader": "^0.13.0",
    "uglifycss": "0.0.21",
    "url-loader": "^0.5.7",
    "webpack": "1.13.0"
  },
  "author": "Sideways NYC <info@sideways-nyc.com>",
  "license": "ISC",
  "devDependencies": {
    "copy-webpack-plugin": "^2.1.3",
    "file-loader": "^0.9.0",
    "ignore-loader": "^0.1.1",
    "imagemin-cli": "^2.1.0",
    "jscs": "^3.0.3",
    "jscs-loader": "^0.3.0",
    "jshint": "^2.9.2",
    "jshint-loader": "^0.8.3",
    "resolve-url": "^0.2.1",
    "uglify-js": "^2.6.2",
    "url-loader": "^0.5.7"
  }
}
